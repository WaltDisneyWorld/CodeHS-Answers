function start(){
	var boxes = readInt("How many boxes did you sell?");
	var badges = readInt ("How many badges do you have?");
	var hoursVolunteered = readInt("How many hours have you volunteerd");
	var goldStatus = boxes >= 100 || boxes >= 50 && badges >= 10 && hoursVolunteered >= 25;
	println("Is gold status?" + goldStatus);
}
