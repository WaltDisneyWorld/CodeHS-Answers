var SENTINEL = 0;

// Prints whether the entered number is even or odd
function start(){
	var num = "hi";
	while(true){
	    num = isEven(readInt("Enter a number:"));
	    if(num=="Done!"){
	        println(num);
	        break;
	    }else{
	        println (num);
	    }
	}
}


function isEven(x){
    var even = x;
    if(even==0){
        even = "Done!";
    }else{
        if (even % 2 ==0 ){
            even = "Even";
        }else{
            if(even % 2!=0){
                even = "Odd";
            }
        }
    }
    return even;
}
